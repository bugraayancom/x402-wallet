# Application
VITE_APP_TITLE=x402 Wallet Studio
VITE_APP_LOGO=/logo.png

# API Configuration
VITE_API_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000

# WalletConnect
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# RPC Endpoints
VITE_ALCHEMY_API_KEY=your_alchemy_api_key
VITE_INFURA_API_KEY=your_infura_api_key

# Chain RPCs (fallback to public RPCs if not provided)
VITE_BASE_RPC_URL=https://mainnet.base.org
VITE_ETHEREUM_RPC_URL=https://eth.llamarpc.com
VITE_POLYGON_RPC_URL=https://polygon-rpc.com
VITE_ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc

# x402 Protocol
VITE_X402_GATEWAY_URL=https://gateway.x402.org
VITE_X402_NETWORK_ID=x402-mainnet
VITE_X402_FACILITATOR_NODES=https://facilitator-1.x402.org,https://facilitator-2.x402.org

# Analytics (Optional)
VITE_ANALYTICS_ENDPOINT=
VITE_ANALYTICS_WEBSITE_ID=

# Sentry Error Tracking (Optional)
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_TESTNET=true
VITE_ENABLE_GAS_OPTIMIZER=true
VITE_ENABLE_MULTI_WALLET=true

# Development
NODE_ENV=development
PORT=3000
